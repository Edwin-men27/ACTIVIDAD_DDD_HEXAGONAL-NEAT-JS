#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Edwin/Desktop/EMENDOZA/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts_9222098e2ae5ae565763aa5486a4434f/node_modules/typeorm/node_modules:/mnt/c/Users/Edwin/Desktop/EMENDOZA/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts_9222098e2ae5ae565763aa5486a4434f/node_modules:/mnt/c/Users/Edwin/Desktop/EMENDOZA/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Edwin/Desktop/EMENDOZA/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts_9222098e2ae5ae565763aa5486a4434f/node_modules/typeorm/node_modules:/mnt/c/Users/Edwin/Desktop/EMENDOZA/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts_9222098e2ae5ae565763aa5486a4434f/node_modules:/mnt/c/Users/Edwin/Desktop/EMENDOZA/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
